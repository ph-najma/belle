  <script>
    document.addEventListener("DOMContentLoaded", function () {
      const forms = document.querySelectorAll(
        "form[action='/userproductlist']"
      );
      const urlParams = new URLSearchParams(window.location.search);

      // Function to add hidden inputs to a form
      function addHiddenInputs(form, params) {
        for (const key in params) {
          if (params[key]) {
            const hiddenInput = document.createElement("input");
            hiddenInput.type = "hidden";
            hiddenInput.name = key;
            hiddenInput.value = params[key];
            form.appendChild(hiddenInput);
          }
        }
      }

      // Clear Filters Button click event
      document
        .getElementById("clearFiltersBtn")
        .addEventListener("click", function (event) {
          event.preventDefault(); // Prevent form submission

          // Reset all filter inputs
          document.getElementById("brandForm").reset(); // Reset brand checkboxes
          document
            .querySelectorAll(".size-swacthes button")
            .forEach(function (button) {
              button.classList.remove("active"); // Remove active state from size buttons
            });
          document.getElementById("min_price").value = ""; // Reset minimum price input
          document.getElementById("max_price").value = ""; // Reset maximum price input

          // Remove all query parameters
          urlParams.forEach((value, key) => urlParams.delete(key));

          // Update form action with new query parameters (empty)
          forms.forEach((form) => {
            form.action = "/userproductlist";
            form.innerHTML = ""; // Remove all children
          });

          // Submit the form
          forms[0].submit(); // Submit the first form found
        });

      // Add hidden inputs to forms
      forms.forEach((form) => {
        addHiddenInputs(form, Object.fromEntries(urlParams.entries()));
      });

      // Function to handle sorting change
      function handleSortChange(selectElement) {
        const form = document.getElementById("sortForm");
        const urlParams = new URLSearchParams(window.location.search);

        // Clear existing SortBy parameter
        urlParams.delete("SortBy");

        // Set new SortBy parameter if selected
        if (selectElement.value) {
          urlParams.set("SortBy", selectElement.value);
        }

        // Update form action with new query parameters
        form.action = "/userproductlist?" + urlParams.toString();

        // Submit the form
        form.submit();
      }

      // Submit the form when a brand checkbox is clicked
      const brandCheckboxes = document.querySelectorAll('input[name="brand"]');
      brandCheckboxes.forEach(function (checkbox) {
        checkbox.addEventListener("change", function () {
          document.getElementById("brandForm").submit();
        });
      });

      // Initialize the price slider (assuming you have jQuery loaded)
      $(function () {
        $("#slider-range").slider({
          range: true,
          min: 0,
          max: 1000, // Set your maximum price limit here
          values: [75, 300], // Default values
          slide: function (event, ui) {
            $("#amount_min").text("$" + ui.values[0]);
            $("#amount_max").text("$" + ui.values[1]);
            $("#min_price").val(ui.values[0]);
            $("#max_price").val(ui.values[1]);
          },
        });
        $("#amount_min").text("$" + $("#slider-range").slider("values", 0));
        $("#amount_max").text("$" + $("#slider-range").slider("values", 1));
      });
    });
  </script>

